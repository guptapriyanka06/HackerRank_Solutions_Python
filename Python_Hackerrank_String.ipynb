   "source": [
    "# String-Immutable\n",
    "to mutate first convert it into list and then change \n",
    "or break the string with slicing opr to put /change char at particular position"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
  
   "source": [
    "def mutate_string(string, position, character):\n",
    "    p=\"\"\n",
    "    p=string[:position]+character+string[position+1:]\n",
    "    return p\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    s = input()\n",
    "    i, c = input().split()\n",
    "    s_new = mutate_string(s, int(i), c)\n",
    "    print(s_new)"
   ]
  },
